name: BiliBili 重定向 MCDN / PCDN 到正规 CDN
desc: 加载更流畅的 BiliBili 更值得干杯🍻！
http:
  force-http-engine:
    - '*.etax.*'
  mitm:
    - "*.etax.*"
  script:
# 嘀嗒出行
    - match: *.etax.*
      name: your-fancy-script-etax
      type: response
      require-body: true
      timeout: 10
    
script-providers:
# 嘀嗒出行
  your-fancy-script-etax:
    url: https://your-fancy-script.com/your-fancy-script.js
    interval: 86400
