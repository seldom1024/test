name: BiliBili 重定向 MCDN / PCDN 到正规 CDN
desc: 加载更流畅的 BiliBili 更值得干杯🍻！
http:
  force-http-engine:
    - '*etax.chinatax.gov.cn:*'
  mitm:
    - '*.etax.chinatax.gov.cn:*'
  script:
    - match: .*etax.*
      name: your-fancy-script-etax
      type: response
      require-body: true
      timeout: 10
    
script-providers:
  your-fancy-script-etax:
    url: https://raw.githubusercontent.com/seldom1024/test/master/test.js
    interval: 86400
